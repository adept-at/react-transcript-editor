{
  "name": "@adept-at/react-transcript-editor",
  "description": "A React component to make transcribing audio and video easier and faster - shout out to https://github.com/bbc/react-transcript-editor.",
  "version": "0.0.3",
  "keywords": [
    "transcript",
    "transcriptions",
    "react",
    "speech",
    "speech to text",
    "textAV"
  ],
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "exports": {
    ".": {
      "import": "./build/mjs/index.js",
      "require": "./build/cjs/index.js"
    }
  },
  "files": [
    "/build"
  ],
  "types": "/build/cjs/index.d.ts",
  "scripts": {
    "build:tsc": "tsc -p tsconfig.build.json",
    "build:tsc:esm": "tsc -p tsconfig.build.esm.json",
    "build:watch": "tsc -p tsconfig.build.esm.json --incremental --watch",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "npm test -- --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "validate": "npm run lint && npm test && npm run build",
    "build": "rimraf build && npm run build:tsc && npm run build:tsc:esm",
    "type-check": "tsc --noEmit",
    "test:ci": "jest --ci",
    "lint:fix-styles": "prettier-stylelint --write 'packages/**/*.{css,scss}'",
    "build:component": "webpack --config webpack.config.js",
    "build:storybook": "rimraf build && build-storybook -c .storybook -o build",
    "build:storybook:serve": "npx serve build",
    "deploy:ghpages": "rimraf build && npm run build:storybook && gh-pages -d build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.5",
    "@mdi/js": "^7.3.67",
    "@mdi/react": "^1.6.1",
    "babel-polyfill": "^6.26.0",
    "difflib": "^0.2.4",
    "docx": "^4.7.1",
    "draft-js": "^0.11.7",
    "everpolate": "0.0.3",
    "mousetrap": "1.5.2",
    "number-to-words": "^1.2.4",
    "prop-types": "^15.6.2",
    "react-fast-compare": "^2.0.4",
    "react-keyboard-shortcuts": "^1.1.3",
    "react-simple-tooltip": "^2.6.1",
    "sbd": "^1.0.15",
    "smpte-timecode": "^1.2.3"
  },
  "peerDependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "@types/css-mediaquery": "^0.1.1",
    "@types/draft-js": "^0.11.13",
    "@types/jest": "^25.2.3",
    "@types/jest-axe": "^3.5.1",
    "@types/node": "^12.12.54",
    "@types/pluralize": "0.0.29",
    "@types/qs": "^6.9.5",
    "@types/react": "^16.13.1",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "@types/react-dom": "^16.9.8",
    "@types/react-pdf": "^5.0.0",
    "@types/react-syntax-highlighter": "^13.5.0",
    "@types/styled-components": "^5.1.25",
    "@types/video.js": "^7.3.10",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "css-loader": "^2.1.1",
    "enzyme": "^3.10.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jest-dom": "^2.1.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.2.0",
    "gh-pages": "^2.1.1",
    "husky": "^1.1.3",
    "jest": "^26.6.3",
    "jest-axe": "^4.1.0",
    "jest-styled-components": "^7.0.3",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.14.1",
    "prettier-stylelint": "^0.4.2",
    "react-testing-library": "^5.2.3",
    "regenerator-runtime": "^0.12.1",
    "rimraf": "^2.7.1",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "stylelint-config-standard": "^18.3.0",
    "typescript": "^4.6.3",
    "why-did-you-update": "^1.0.6"
  },
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adept-at/react-transcript-editor/issues"
  },
  "homepage": "https://github.com/adept-at/react-transcript-editor",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adept-at/react-transcript-editor.git"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
